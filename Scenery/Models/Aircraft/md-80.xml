<?xml version="1.0"?>
<PropertyList>
  <path>Models/Aircraft/md-80.ac</path>
  <animation>
    <type>range</type>
    <min-m>0</min-m>
    <max-property>sim/rendering/static-lod/bare</max-property>
  </animation>
  <animation>
    <type>material</type>
    <object-name>lhfuse</object-name>
    <object-name>no1pylon</object-name>
    <object-name>no2engintake</object-name>
    <object-name>no1pylon.001</object-name>
    <object-name>disklha</object-name>
    <object-name>lhstab</object-name>
    <object-name>no1engintake</object-name>
    <object-name>reverserrh</object-name>
    <object-name>gear</object-name>
    <object-name>rhtail</object-name>
    <object-name>lhwing</object-name>
    <object-name>rhfuse</object-name>
    <object-name>lhstab.001</object-name>
    <object-name>no2eng</object-name>
    <object-name>lhtail</object-name>
    <object-name>diskrh</object-name>
    <object-name>reverserlh</object-name>
    <object-name>lhwing.001</object-name>
    <object-name>no1eng</object-name>
    <condition>
      <greater-than>
        <property>/sim/time/sun-angle-rad</property>
        <value>1.57</value>
      </greater-than>
    </condition>
    <emission>
      <red>   0.4 </red>
      <green> 0.4 </green>
      <blue>  0.4 </blue>
    </emission>
  </animation>

  <animation>
    <type>material</type>
    <object-name>lhfuse</object-name>
    <object-name>no1pylon</object-name>
    <object-name>no2engintake</object-name>
    <object-name>no1pylon.001</object-name>
    <object-name>disklha</object-name>
    <object-name>lhstab</object-name>
    <object-name>no1engintake</object-name>
    <object-name>reverserrh</object-name>
    <object-name>gear</object-name>
    <object-name>rhtail</object-name>
    <object-name>lhwing</object-name>
    <object-name>rhfuse</object-name>
    <object-name>lhstab.001</object-name>
    <object-name>no2eng</object-name>
    <object-name>lhtail</object-name>
    <object-name>diskrh</object-name>
    <object-name>reverserlh</object-name>
    <object-name>lhwing.001</object-name>
    <object-name>no1eng</object-name>
    <condition>
      <less-than-equals>
        <property>/sim/time/sun-angle-rad</property>
        <value>1.57</value>
      </less-than-equals>
    </condition>
    <emission>
      <red>   0.0 </red>
      <green> 0.0 </green>
      <blue>  0.0 </blue>
    </emission>
  </animation>

</PropertyList>
